msgstr "+55 (48) 3206-6033"

#: comunicacao/templates/contato/contato.html:22
msgid "e do email contato@shiftit.com.br"
msgstr "and email contato@shiftit.com.br"

#: comunicacao/templates/contato/contato.html:28
